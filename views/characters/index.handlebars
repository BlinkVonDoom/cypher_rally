<h1>characters</h1>
<div class="row">
  {{#each characters}}
    <div class="col s12 m4">
      <div class="card">
        <div class="card-image">
        </div>
        <div class="card-content center-align">
          <h5>{{name}}</h5>
          <br/>
          <div class="chip">
            <img src="{{author.image}}">{{author.firstName}} {{author.lastName}}
          </div>
        </div>
        <div class="card-action center-align">
          <a class="btn grey" href="/characters/show/{{id}}">read
          </a>
        </div>
      </div>
    </div>
  {{/each}}
</div>
